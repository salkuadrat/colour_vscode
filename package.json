{
	"name": "fluttercolour",
	"displayName": "fluttercolour",
	"description": "Visual Studio Code Extension to show color of Colour",
	"version": "0.0.1",
	"publisher": "salkuadrat",
	"engines": {
		"vscode": "^1.49.0"
	},
	"extensionKind": [
		"workspace"
	],
	"license": "SEE LICENSE IN LICENSE",
	"bugs": {
		"url": "https://github.com/salkuadrat/colour_vscode/issues"
	},
	"homepage": "https://github.com/salkuadrat/colour_vscode",
	"repository": {
		"type": "git",
		"url": "https://github.com/salkuadrat/colour_vscode.git"
	},
	"categories": [
		"Other"
	],
	"languages": [
		{
			"id": "dart",
			"extensions": [
				".dart"
			],
			"aliases": [
				"dart"
			]
		}
	],
	"activationEvents": [
		"onLanguage:dart",
		"workspaceContains:**/*.dart"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": []
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.49.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^14.0.27",
		"eslint": "^7.9.0",
		"@typescript-eslint/eslint-plugin": "^4.1.1",
		"@typescript-eslint/parser": "^4.1.1",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.0.2",
		"vscode-test": "^1.4.0"
	}
}
